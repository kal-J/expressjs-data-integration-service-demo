{
	"name": "express-typescript-boilerplate",
	"version": "1.0.14",
	"description": "Data integration service for an e-commerce platform.",
	"author": "Kalujja Eric",
	"repository": "https://github.com/kal-J/expressjs-data-integration-service-demo",
	"license": "MIT",
	"main": "src/index.ts",
	"private": true,
	"scripts": {
		"build": "tsc && tsup",
		"start:dev": "node --import=tsx --watch src/index.ts",
		"start:prod": "node dist/index.js",
		"check": "biome check --write",
		"test": "vitest run",
		"test:cov": "vitest run --coverage",
		"swagger:gen": "node --import=tsx src/docs/swaggerGen.ts"
	},
	"dependencies": {
		"@asteasolutions/zod-to-openapi": "7.3.4",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.3",
		"cors": "2.8.5",
		"csv-parser": "^3.2.0",
		"dotenv": "17.2.1",
		"express": "5.1.0",
		"express-rate-limit": "8.0.1",
		"helmet": "8.1.0",
		"http-status-codes": "2.3.0",
		"mongoose": "^9.1.5",
		"multer": "^2.0.2",
		"pino": "9.9.0",
		"pino-http": "10.5.0",
		"reflect-metadata": "^0.2.2",
		"swagger-ui-express": "5.0.1",
		"zod": "3.25.76"
	},
	"devDependencies": {
		"@biomejs/biome": "2.1.4",
		"@types/cors": "2.8.19",
		"@types/express": "5.0.3",
		"@types/mongoose": "^5.11.97",
		"@types/multer": "^2.0.0",
		"@types/supertest": "6.0.3",
		"@types/swagger-ui-express": "4.1.8",
		"swagger-autogen": "2.23.5",
		"@vitest/coverage-v8": "3.2.4",
		"pino-pretty": "13.1.1",
		"supertest": "7.1.4",
		"tsup": "8.5.0",
		"tsx": "4.20.4",
		"typescript": "5.9.2",
		"vite-tsconfig-paths": "5.1.4",
		"vitest": "3.2.4"
	},
	"tsup": {
		"entry": [
			"src/index.ts"
		],
		"outDir": "dist",
		"format": [
			"esm",
			"cjs"
		],
		"target": "es2020",
		"sourcemap": true,
		"clean": true,
		"dts": true,
		"splitting": false,
		"skipNodeModulesBundle": true
	},
	"packageManager": "pnpm@10.14.0"
}
